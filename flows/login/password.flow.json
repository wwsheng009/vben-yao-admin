{
  "label": "Password Login",
  "version": "1.0.0",
  "description": "Password Login",
  "nodes": [
    {
      "name": "User",
      "process": "models.contact.user.Get",
      "args": [
        {
          "select": [
            "id",
            "name",
            "password"
          ],
          "wheres": [
            {
              "column": "name",
              "value": "?:$username"
            }
          ],
          "limit": 1
        }
      ]
    },
    {
      "name": "Validate Passwor",
      "process": "xiang.helper.PasswordValidate",
      "args": [
        "{{$password}}",
        "{{$res.User.0.password}}"
      ]
    },
    {
      "name": "Response",
      "process": "flows.login.Token",
      "args": [
        "{{$res.User.0.id}}"
      ]
    },
    {
      "name": "result",
      "process": "scripts.return.Success",
      "args": [
        "{{$res.Response}}"
      ]
    }
  ],
  "output": "{{$res.result}}"
}